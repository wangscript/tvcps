<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!--客户调查start -->       
	<bean name="/onlineSurvey" class="com.baize.ccms.plugin.onlinesurvey.web.action.OnlineSurveyAction" scope="prototype">
		<property name="baseBiz" ref="OnlineSurvey-OnlineSurveyBiz"/>
	</bean>                                        
	<bean id="OnlineSurvey-OnlineSurveyBiz" class="com.baize.ccms.plugin.onlinesurvey.service.OnlineSurveyBiz" scope="singleton">		
		<property name="onlineSurveyService" ref="onlineSurver-onlineSurveyService"/>
	</bean>
	<bean id="onlineSurver-onlineSurveyService" class="com.baize.ccms.plugin.onlinesurvey.service.impl.OnlineSurveyServiceImpl" scope="singleton">
	  	<property name="onlineSurveyDao"  ref="onlineSurver-onlineSurveyDao"/>
	  	<property name="jdbcTemplate" ref="jdbcTemplate" />	
	  	<property name="onlineSurveyContentDao"  ref="onlineSurver-onlineSurveyContentDao"/>
	  	<property name="onlineSurveyAnswerContentDao"  ref="onlineSurver-onlineSurveyAnswerContentDao"/>
	  	<property name="siteDao" ref="sitemanager-siteDao"/>
	  	<property name="templateUnitDao" ref="templatemanager-templateUnitDao" />
	  	<property name="onlinefeedbackContentDao"  ref="onlineSurver-onlinefeedbackContentDao"/>
	</bean>
	<bean id="onlineSurver-onlineSurveyDao"	class="com.baize.ccms.plugin.onlinesurvey.dao.imp.OnlineSurveyDaoImpl" parent="genericDaoImpl" />
	<!--客户调查end -->	
	
	<!--客户详情调查begin-->         
	<bean name="/onlineSurveyConcret" class="com.baize.ccms.plugin.onlinesurvey.web.action.OnlineSurveyContentAction" scope="prototype">
		<property name="baseBiz" ref="OnlineSurvey-OnlineSurveyContent"/>
	</bean>                                                               
	<bean id="OnlineSurvey-OnlineSurveyContent" class="com.baize.ccms.plugin.onlinesurvey.service.OnlineSurveyContentBiz" scope="singleton">		
		<property name="onlineSurveyContentService" ref="onlineSurver-onlineSurveyContentService"/>
	</bean>
	<bean id="onlineSurver-onlineSurveyContentService" class="com.baize.ccms.plugin.onlinesurvey.service.impl.OnlineSurveyContentServiceImpl" scope="singleton">
		<property name="onlineSurveyContentDao"  ref="onlineSurver-onlineSurveyContentDao"/>
		<property name="onlineSurveyAnswerContentDao"  ref="onlineSurver-onlineSurveyAnswerContentDao"/>
		<property name="siteDao" ref="sitemanager-siteDao"/>
	</bean>
	<bean id="onlineSurver-onlineSurveyContentDao" class="com.baize.ccms.plugin.onlinesurvey.dao.imp.OnlineSurveyContentDaoImpl" parent="genericDaoImpl" />
	<!--客户详情调查end-->
	
	<!--客户详情调查结果begin-->
	<bean name="/onlineSurveyConcretQuestion" class="com.baize.ccms.plugin.onlinesurvey.web.action.OnlineSurveyContentAnswerAction" scope="prototype">
		<property name="baseBiz" ref="OnlineSurvey-OnlineSurveyContentAnswer"/>
	</bean>                                                               
	<bean id="OnlineSurvey-OnlineSurveyContentAnswer" class="com.baize.ccms.plugin.onlinesurvey.service.OnlineSurveyContentAnswerBiz" scope="singleton">		
		<property name="onlineSurveyContentAnswerService" ref="onlineSurver-onlineSurveyContentAnswerService"/>
	</bean>
	<bean id="onlineSurver-onlineSurveyContentAnswerService" class="com.baize.ccms.plugin.onlinesurvey.service.impl.OnlineSurveyContentAnswerServiceImpl" scope="singleton">
		<property name="onlineSurveyAnswerContentDao"  ref="onlineSurver-onlineSurveyAnswerContentDao"/>
	</bean>         
	<bean id="onlineSurver-onlineSurveyAnswerContentDao" class="com.baize.ccms.plugin.onlinesurvey.dao.imp.OnlineSurveyContentAnswerDaoImpl" parent="genericDaoImpl" />
	<!--客户详情调查结果end-->

	<!--客户详情调查结果反馈意见begin-->
	<bean name="/onlinefeedbackContent" class="com.baize.ccms.plugin.onlinesurvey.web.action.OnlinefeedbackContentAction" scope="prototype">
		<property name="baseBiz" ref="OnlineSurvey-OnlinefeedbackContent"/>                                   
	</bean>                                                               
	<bean id="OnlineSurvey-OnlinefeedbackContent" class="com.baize.ccms.plugin.onlinesurvey.service.OnlinefeedbackContentBiz" scope="singleton">		
		<property name="onlinefeedbackContentService" ref="onlineSurver-onlinefeedbackContentService"/>
	</bean>
	<bean id="onlineSurver-onlinefeedbackContentService" class="com.baize.ccms.plugin.onlinesurvey.service.impl.OnlinefeedbackContentServiceImpl" scope="singleton">
	  	<property name="onlinefeedbackContentDao"  ref="onlineSurver-onlinefeedbackContentDao"/>        
	</bean>  
	<bean id="onlineSurver-onlinefeedbackContentDao" class="com.baize.ccms.plugin.onlinesurvey.dao.imp.OnlinefeedbackContentDaoImpl" parent="genericDaoImpl" />
	<!--客户详情调查结果反馈意见结果end-->
	
	<bean name="/outOnlineSurvery" class="com.baize.ccms.plugin.onlinesurvey.web.action.OutOnlineSurveryAction" scope="singleton">  
    	<property name="onlineSurveyService" ref="onlineSurver-onlineSurveyService" />
  	</bean>  
</beans>