<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	version="2.4"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee   http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
	<display-name>ccms</display-name>
	<welcome-file-list>
		<welcome-file>/setup/login.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<description></description>
		<display-name>SetupServlet</display-name>
		<servlet-name>SetupServlet</servlet-name>
		<servlet-class>com.baize.ccms.biz.setupmanager.servlet.SetupServlet</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name> upload</display-name>
		<servlet-name>upload</servlet-name>
		<servlet-class> com.baize.ccms.biz.setupmanager.servlet.upload</servlet-class>
	</servlet>
	<servlet>
		<description>
		</description>
		<display-name>
		CheckInit</display-name>
		<servlet-name>CheckInit</servlet-name>
		<servlet-class>
		com.baize.ccms.biz.setupmanager.servlet.CheckInit</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SetupServlet</servlet-name>
		<url-pattern>/SetupServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>upload</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>CheckInit</servlet-name>
		<url-pattern>/CheckInit</url-pattern>
	</servlet-mapping>
	<jsp-config>
		<taglib>
			<taglib-uri>/WEB-INF/tlds/complat.tld</taglib-uri>
			<taglib-location>/WEB-INF/tlds/complat.tld</taglib-location>
		</taglib>
		<taglib>
			<taglib-uri>/WEB-INF/tlds/ccms.tld</taglib-uri>
			<taglib-location>/WEB-INF/tlds/ccms.tld</taglib-location>
		</taglib>
	</jsp-config>
	<session-config>
		<session-timeout>100</session-timeout>
	</session-config>
</web-app>